datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  code      String   @unique
  name      String
  image     String
  birthDate DateTime
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}
